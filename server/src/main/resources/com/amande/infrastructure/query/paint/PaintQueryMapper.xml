<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.amande.infrastructure.query.paint.PaintQueryMapper">
  <select id="selectAll" resultType="com.amande.infrastructure.query.paint.PaintQueryRecord">
    SELECT id,
           productnumber as productNumber,
           colorname as colorName,
           colorcode as colorCode
    FROM vallejocolors
  </select>
</mapper>