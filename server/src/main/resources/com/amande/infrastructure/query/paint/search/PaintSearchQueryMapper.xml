<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.amande.infrastructure.query.paint.search.PaintSearchQueryMapper">
  <select id="select" resultType="com.amande.infrastructure.query.paint.search.PaintSearchQueryRecord">
    SELECT aggregate_id id,
           last_version lastVersion,
           color_name   colorName,
           color_code   colorCode
    FROM query.paint
  </select>
</mapper>